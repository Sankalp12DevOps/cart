pipeline{

environment{

}

stages{

    stage('lint checks'){
        steps{
        '''
    sh "echo Lint checking"
    sh "npm install jslint"
    sh "node_modules/jslint/bin/jslint.js server.js"
    '''
        }
    }

    stage('install dependencies')
    steps{
    
    sh "npm install"
    }


}


}