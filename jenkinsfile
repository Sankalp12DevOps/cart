             script{
     nodejs.performLintChecks('Cart')
     nodejs.installDependencies('cart'){
     


        }


    // sh "node_modules/jslint/bin/jslint.js server.js"
    
        }
    }

    stage('install dependencies'){
    steps{
    
    sh "npm install"
    }
    }


}


}